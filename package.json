{
  "name": "vakaravaditajs",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurismikals/vakaravaditajs"
  },
  "license": "MIT",
  "main": "server/index.js",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node static/buildServer/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf static/buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf static/buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server -d static/buildServer --ignore configureStore,render",
    "clean": "rimraf static/buildClient static/buildServer",
    "cm": "git-cz",
    "lint": "eslint --fix src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint",
    "svgMinifyIcons": "mkdirp static/dist/icons-min && svgo -f src/00-assets/icons -o static/dist/icons-min",
    "svgMakeSprite": "spritesh --input static/dist/icons-min --output static/dist/icons-sprite.svg && rimraf static/dist/icons-min",
    "svg": "npm run svgMinifyIcons && npm run svgMakeSprite",
    "images": "node optimize-images.js",
    "sw": "workbox generate:sw"
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.3.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "4.0.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "eslint-loader": "^2.0.0",
    "express": "^4.16.2",
    "fetch-everywhere": "^1.0.5",
    "history": "^4.7.2",
    "imagemin": "^5.3.1",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-webp": "^4.1.0",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "mysql": "^2.15.0",
    "node-env-file": "^0.1.8",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-lazy-load": "^3.0.13",
    "react-redux": "^5.0.5",
    "react-universal-component": "^2.5.0",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.15",
    "redux-first-router-link": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "spritesh": "^1.2.0",
    "svg4everybody": "^2.1.9",
    "svgo": "^1.0.5",
    "transition-group": "^0.0.2",
    "webpack-flush-chunks": "^1.1.22",
    "workbox-cli": "^2.1.2"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.4",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-universal-import": "^1.2.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "bluebird": "^3.5.1",
    "commitizen": "^2.9.6",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-css-chunks-webpack-plugin": "^2.0.15",
    "flow-bin": "^0.57.2",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "mkdirp": "^0.5.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-math": "^0.0.10",
    "prettier": "^1.4.4",
    "react-hot-loader": "^3.0.0",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.1",
    "sugarss": "^1.0.1",
    "travis-github-status": "^1.4.0",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.1.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
